{
  "name": "@maeum/maeum",
  "version": "0.0.1",
  "description": "maeum boilerplate",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "artifact": "just --config ./.config/just.config.ts artifact",
    "build": "just --config ./.config/just.config.ts build",
    "pack": "just --config ./.config/just.config.ts pack",
    "clean": "just --config ./.config/just.config.ts clean",
    "dev": "nodemon --config ./.config/nodemon.json",
    "devs": "concurrently \"npm run dev\" \"npm run tjs-w\"",
    "debug": "just --config ./.config/just.config.ts debug",
    "just": "just --config ./.config/just.config.ts",
    "lint": "just --config ./.config/just.config.ts lint",
    "route": "cross-env NODE_ENV=production fast-maker route --config ./.config/.fastmakerrc",
    "start:withoutpm2": "TZ=Asia/Seoul node --enable-source-maps dist/src/app.js",
    "start": "TZ=Asia/Seoul ./node_modules/pm2/bin/pm2-runtime -s start dist/.config/pm2.config.js > /dev/null",
    "test": "cross-env DEBUG=maeum:* jest --detectOpenHandles --runInBand",
    "tjs": "tjscli tsj -i --overwrite --config ./.config/.tjsclirc",
    "tjs-w": "tjscli tsj-w --config ./.config/.tjsclirc --overwrite --watch .",
    "tsc": "just --config ./.config/just.config.ts tsc",
    "tsc-watch": "just --config ./.config/just.config.ts tsc-watch",
    "typeorm": "cross-env DISABLE_RELATION_MODEL=on ts-node -r tsconfig-paths/register -r scripts/typeorm ./node_modules/typeorm/cli.js",
    "uid": "just --config ./.config/just.config.ts uid"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maeumjs/maeum.git"
  },
  "author": "ByungJoon Lee",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/archiver": "^5.3.1",
    "@types/debug": "^4.1.7",
    "@types/figlet": "^1.5.4",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "@types/node-polyglot": "^2.4.2",
    "@types/shelljs": "^0.8.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/eslint-plugin-tslint": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "archiver": "^5.3.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-maker": "^1.0.1",
    "figlet": "^1.5.2",
    "jest": "^28.1.2",
    "just-scripts": "^2.0.0",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "simple-tjscli": "^1.0.8",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "@fastify/cors": "^8.0.0",
    "@fastify/swagger": "^7.4.1",
    "accept-language": "^3.0.18",
    "ajv": "^8.11.0",
    "change-case": "^4.1.2",
    "colors": "^1.4.0",
    "dayjs": "^1.11.3",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "fastify": "^4.2.0",
    "fastify-plugin": "^3.0.1",
    "http-status-codes": "^2.2.0",
    "jin-curlize": "^0.5.0",
    "jin-frame": "^1.0.0",
    "json-schema": "^0.4.0",
    "jsonc-parser": "^3.0.0",
    "my-easy-fp": "^0.13.0",
    "my-node-fp": "^0.7.0",
    "node-polyglot": "^2.4.2",
    "ts-essentials": "^9.1.2",
    "urlcat": "^2.0.4",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  }
}
